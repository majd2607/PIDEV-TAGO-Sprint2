{% extends 'baseBack.html.twig' %}

{% block title %}Maintenances
{% endblock %}

{% block body %}
<div class="container-xxl flex-grow-1 container-p-y">
<h1>Maintenance Details</h1>
<a type="button" class="btn btn-primary mb-3" href="{{ path('app_maintenances_edit', {'id': maintenance.id}) }}">Edit</a>
{{ include('maintenances/_delete_form.html.twig') }}
	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ maintenance.id }}</td>
			</tr>
			<tr>
				<th>Date</th>
				<td>{{ maintenance.date ? maintenance.date|date('Y-m-d') : '' }}</td>
			</tr>
			<tr>
				<th>Type</th>
				<td>{{ maintenance.type }}</td>
			</tr>
			<tr>
				<th>Status</th>
				<td>{{ maintenance.status }}</td>
			</tr>
			{#<tr>
				<th>Archive</th>
				<td>{{ maintenance.archive ? 'Yes' : 'No' }}</td>
			</tr>#}
		</tbody>
	</table>
</div>

<a href="{{ path('app_maintenances_index') }}">back to list</a>
{% endblock %}


